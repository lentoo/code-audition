{
  "name": "code-audition",
  "version": "0.1.4",
  "private": true,
  "description": "码上面试，主要是解决人们刷面试题的刚需，使所有的人能“随时随地”刷题",
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "update": "taro update self",
    "doctor": "taro doctor",
    "info": "taro info",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "commit": "git-cz",
    "release": "release-it",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "author": "",
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@tarojs/async-await": "1.3.12",
    "@tarojs/cli": "1.3.12",
    "@tarojs/components": "1.3.12",
    "@tarojs/redux": "1.3.12",
    "@tarojs/redux-h5": "1.3.12",
    "@tarojs/router": "1.3.12",
    "@tarojs/taro": "1.3.12",
    "@tarojs/taro-alipay": "1.3.12",
    "@tarojs/taro-h5": "1.3.12",
    "@tarojs/taro-swan": "1.3.12",
    "@tarojs/taro-tt": "1.3.12",
    "@tarojs/taro-weapp": "1.3.12",
    "classnames": "^2.2.6",
    "global": "^4.3.2",
    "nerv-devtools": "^1.4.3",
    "nervjs": "^1.4.3",
    "promise-polyfill": "^8.1.3",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "taro-skeleton": "^1.0.10",
    "taro-ui": "^2.2.1"
  },
  "devDependencies": {
    "@tarojs/plugin-babel": "1.3.12",
    "@tarojs/plugin-csso": "1.3.12",
    "@tarojs/plugin-sass": "1.3.12",
    "@tarojs/plugin-uglifyjs": "1.3.12",
    "@tarojs/webpack-runner": "1.3.12",
    "@types/classnames": "^2.2.9",
    "@types/react": "^16.4.8",
    "@types/webpack-env": "^1.13.6",
    "babel-eslint": "^8.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-jsx-stylesheet": "^0.6.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "commitizen": "^4.0.3",
    "conventional-changelog-cli": "^2.0.23",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^4.19.1",
    "eslint-config-taro": "1.3.12",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-taro": "1.3.12",
    "git-cz": "^3.2.1",
    "release-it": "^12.3.5"
  }
}
